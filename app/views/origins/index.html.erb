<h1> All the origin of kopis </h1>

<p><%= link_to 'Add new origin', new_origin_path %></p>

<% @origin.each do |origin| %>

	<p>
		Location: <%= origin.location %><br>
		Number: <%= origin.phone_number %>
	</p>
	<p><%= link_to 'Show', origin_path(origin) %></p>
	<p><%= link_to 'Edit', edit_origin_path(origin) %></p>
	<p><%= button_to 'Destroy', origin_path(origin),
              method: :delete,
              data: { confirm: 'Are you sure? REALLLY??????' } %>
